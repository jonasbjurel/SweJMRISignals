  <aspecttable>Sweden-3HMS</aspecttable>
  <name>Three head restricted proceed searchlight signal</name>
  <reference>June 2019 signal rules</reference>
  <description>Appearances for a three head restricted proceed searchlight signal used in Automatic Block, CTC and Interlocking Systems</description>
  <appearances>
		<appearance>
			<aspectname>Stopp</aspectname>
			<show>dark</show>
			<show>red</show>
			<show>dark</show>
			<reference>Stopp</reference>
			<imagelink type="aspects">../../../resources/icons/smallschematics/aspects/Sweden-3HMS/SL-3/Stop-small.gif</imagelink>
		</appearance>
	  
		<appearance>
			<aspectname>Kor 80 - vanta</aspectname>
			<show>green</show>
			<show>dark</show>
			<show>dark</show>
			<reference>Kor 80 - vanta</reference>
			<imagelink type="aspects">../../../resources/icons/smallschematics/aspects/Sweden-3HMS/SL-3/Proceed-80-small.gif</imagelink>
		</appearance>
	  
		<appearance>
			<aspectname>Kor 40 - vanta</aspectname>
			<show>green</show>
			<show>dark</show>
			<show>green</show>
			<reference>Kor 40 - vanta</reference>
			<imagelink type="aspects">../../../resources/icons/smallschematics/aspects/Sweden-3HMS/SL-3/Proceed-40-caution-small.gif</imagelink>
		</appearance>
  </appearances>

	<specificappearances>
		<danger>
			<aspect>Stopp</aspect>
		</danger>
		<permissive>
			<aspect>Stopp</aspect>
		</permissive>
		<held>
			<aspect>Stopp</aspect>
		</held>
	</specificappearances>

  <aspectMappings>
    <!-- Start of Aspect Mapping -->
    <aspectMapping>
      <advancedAspect>Stopp</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
	    <ourAspect>Kor 40 - vanta</ourAspect>
	    <ourAspect>Stopp</ourAspect>
    </aspectMapping>

	  <aspectMapping>
		  <advancedAspect>Stopp - vanta - begar tillstand</advancedAspect>
		  <ourAspect>Kor 80 - vanta</ourAspect>
		  <ourAspect>Kor 40 - vanta</ourAspect>
		  <ourAspect>Stopp</ourAspect>
	  </aspectMapping>

	  <aspectMapping>
		  <advancedAspect>Stopp - hinder - vanta - begar tillstand</advancedAspect>
		  <ourAspect>Kor 80 - vanta</ourAspect>
		  <ourAspect>Kor 40 - vanta</ourAspect>
		  <ourAspect>Stopp</ourAspect>
	  </aspectMapping>

	  <aspectMapping>
		  <advancedAspect>Stopp - manuell vaxling - vanta - begar tillstand</advancedAspect>
		  <ourAspect>Kor 80 - vanta</ourAspect>
		  <ourAspect>Kor 40 - vanta</ourAspect>
		  <ourAspect>Stopp</ourAspect>
	  </aspectMapping>
	  
    <aspectMapping>
      <advancedAspect>Kor 80 - vanta</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
	    <ourAspect>Kor 40 - vanta</ourAspect>
	    <ourAspect>Stopp</ourAspect>
    </aspectMapping>
	  
		<aspectMapping>
      <advancedAspect>Kor 40 - vanta</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
			<ourAspect>Kor 40 - vanta</ourAspect>
			<ourAspect>Stopp</ourAspect>
    </aspectMapping>
	  
    <aspectMapping>
      <advancedAspect>Kor 80 - vanta - kor 80</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
			<ourAspect>Kor 40 - vanta</ourAspect>
			<ourAspect>Stopp</ourAspect>
    </aspectMapping>
	  
    <aspectMapping>
      <advancedAspect>Kor 80 - vanta - kor 40</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
			<ourAspect>Kor 40 - vanta</ourAspect>
			<ourAspect>Stopp</ourAspect>
    </aspectMapping>
	  
    <aspectMapping>
      <advancedAspect>Kor 80 - vanta - stopp</advancedAspect>
      <ourAspect>Kor 80 - vanta</ourAspect>
			<ourAspect>Kor 40 - vanta</ourAspect>
			<ourAspect>Stopp</ourAspect>
    </aspectMapping>
  </aspectMappings>
  <!-- End of Aspect Mapping -->
